<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>RED BUS| Start your jouney Now</title>

  <!-- Google Font: Source Sans Pro -->
  <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700&display=fallback">
  <!-- Font Awesome -->
  <link rel="stylesheet" href="static/plugins/fontawesome-free/css/all.min.css">
  <!-- Theme style -->
  <link rel="stylesheet" href="static/css/adminlte.min.css">
  <link rel="stylesheet" href="static/css/custom-autocomplete.css">
</head>
<body class="hold-transition sidebar-mini"> 
<!-- Site wrapper -->
<div class="wrapper">
    <%- include('partials/header')%>
    <%- include('partials/navbar')%>
   
   
    
    <!-- Page Body -->
    <div class="content-wrapper">
        <!-- Content Header (Page header) -->
        <section class="content-header">
          <div class="container-fluid">
            <div class="row mb-2">
              <div class="col-sm-6">
                <h1>Register bus here:-</h1>
              </div>
              
            </div>
          </div><!-- /.container-fluid -->
        </section>
    
        <!-- Main content -->
        <section class="content">
    
          <!-- Default box -->
          <div class="card">
            <div class="card-header">
              <h3 class="card-title">Please Enter Bus Details -></h3>

              </div>
            </div>
            <div class="card-body">
                <h1><%= message %></h1>
               <form action="/bus" method="POST">
                    <div class="row">
                      <div class="col">
                        <input type="text" name="busNumber" autocomplete="off" class="form-control " placeholder="Enter Bus Number">
                      </div>
                    </div>
                    <div class="row">
                      <div class="col">
                        <input type="radio"name="conditioner"  value="AC">
                        <label for="AC">A/C</label>
                        </div>
                        <div class="col">
                        <input type="radio" name="conditioner" value="NON AC">
                        <label for="NON AC">NON A/C</label>
                      </div>
                        
                    </div>
                    <br>
                    <div class="row">
                      <div class="col">
                        <input id="" class="form-control search" name="from" type="text" autocomplete="off" placeholder="from" required>   
                      </div>
                      <div class="col">
                        <input id="" class="form-control to_search" name="to" type="text" autocomplete="off" placeholder="To" required>   
                      </div>
                    </div>
                    <br>


  
                <div id="matchList" class=""></div>
                <div id="matchList2" class=""></div>
                <div class="row">
                  <div class="col">
                    <label for="exampleDataList" class="form-label"> Bus Type<span > :-</span> </label>
                
                    <select class="form-select" aria-label="Default select example" name="type" style="width: 400px;" >
                      <option selected>Select Your Bus....</option>
                       <option value="Tour Bus">Tour Bus </option>
                      <option value="Coach Bus">Coach Bus</option>
                      <option value="Public Transit Bus">Public Transit Bus</option> 
                    </select>
                  </div>
                </div>
                    <button type="submit" class="submit">register.</button>
                    
                </form>
            </div>


            <!-- /.card-body -->
            
            <!-- /.card-footer-->
          </div>
          <!-- /.card -->
    
        </section>
        <!-- /.content -->
      </div>


<%- include('partials/footer')%>

</div>
<!-- jQuery -->
<script src="static/plugins/jquery/jquery.min.js"></script>
<!-- Bootstrap 4 -->
<script src="static/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
<!-- AdminLTE App -->
<script src="static/js/adminlte.min.js"></script>
<!-- AdminLTE for demo purposes -->
<script src="static/js/demo.js"></script>
<script src="/static/js/index.js"></script>

</body>
</html>
